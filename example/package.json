{
  "name": "react-native-audio-waveform-example",
  "description": "Example app for react-native-audio-waveform",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start-reset-cache": "npx react-native start --reset-cache",
    "npm-i": "yarn install",
    "pod-install": "cd ios && pod install && cd ..",
    "jetify": "npx jetify",
    "setup": "npm run npm-i && npm run jetify && npm run pod-install",
    "android-clean": "cd android && ./gradlew cleanBuildCache && cd .. ",
    "android": "react-native run-android",
    "ios": "react-native run-ios"
  },
  "dependencies": {
    "buffer": "^6.0.3",
    "react": "16.13.1",
    "react-native": "0.63.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/runtime": "^7.12.5",
    "babel-plugin-module-resolver": "^4.0.0",
    "metro-react-native-babel-preset": "^0.64.0"
  }
}
